{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "Proyecto del curso Full Stack Web Development",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "clean": "rimraf production",
    "imagemin": "imagemin images/* --out-dir production/images",
    "usemin": "usemin index.html -d production -o production/index.html --htmlmin true && usemin we.html -d production -o production/we.html --htmlmin true && usemin contact.html -d production -o production/contact.html --htmlmin true && usemin prices.html -d production -o production/prices.html --htmlmin true && usemin terms.html -d production -o production/terms.html --htmlmin true",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Angel-Herrarte@bitbucket.org/Angel-Herrarte/guia-hoteles.git"
  },
  "author": "Angel Herrarte",
  "license": "ISC",
  "homepage": "https://bitbucket.org/Angel-Herrarte/guia-hoteles#readme",
  "devDependencies": {
    "browser-sync": "^2.26.12",
    "copyfiles": "^2.4.0",
    "cssmin": "^0.4.3",
    "del": "^6.0.0",
    "grunt": "^1.5.3",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-flatmap": "^1.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-usemin": "^0.3.30",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
